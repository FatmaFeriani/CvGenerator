                <textFieldExpression><![CDATA[$P{degree1}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="20" y="420" width="555" height="20" style="DateStyle"/>
                <textFieldExpression><![CDATA[$P{year1}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="20" y="440" width="555" height="20" style="BodyText"/>
